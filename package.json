    "@typescript-eslint/parser": "^5.44.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.28.0",
    "eslint-plugin-jest": "^27.1.6",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.4",
